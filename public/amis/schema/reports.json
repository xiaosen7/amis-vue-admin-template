                "title": "更新发布",
                "body": {
                  "type": "form",
                  "body": [
                    {
                      "type": "input-text",
                      "name": "reportTitle",
                      "label": "标题",
                      "required": true
                    },
                    {
                      "type": "input-text",
                      "name": "keyword",
                      "label": "关键字",
                      "required": true
                    },
                    {
                      "type": "input-text",
                      "name": "year",
                      "label": "期数",
                      "required": true,
                      "addOn": {
                        "type": "text",
                        "label": "年"
                      }
                    },
                    {
                      "type": "input-text",
                      "name": "issue",
                      "label": "第",
                      "addOn": {
                        "type": "text",
                        "label": "期"
                      }
                    },
                    {
                      "type": "input-text",
                      "name": "total",
                      "label": "总第",
                      "addOn": {
                        "type": "text",
                        "label": "期"
                      }
                    },
                    {
                      "label": "报告类型",
                      "type": "select",
                      "name": "reportType",
                      "options": [
                        {
                          "label": "国内热点报告",
                          "value": "1"
                        },
                        {
                          "label": "国际热点报告",
                          "value": "2"
                        },
                        {
                          "label": "质量周报",
                          "value": "3"
                        }
                      ],
                      "required": true
                    },
                    {
                      "name": "top",
                      "type": "radios",
                      "label": "置顶",
                      "options": [
                        {
                          "label": "否",
                          "value": "0"
                        },
                        {
                          "label": "是",
                          "value": "1"
                        }
                      ]
                    }
                  ],
                  "api": "patch:/json-server/reports/${id}"
                }
              },
              "id": "u:c6fd5f229780"
            }
          ]
        },
        {
          "label": "操作",
